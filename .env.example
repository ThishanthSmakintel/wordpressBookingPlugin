# WordPress Test Environment Variables
# Copy this file to .env and fill in your credentials

# WordPress Admin Credentials (for automated tests)
WP_TEST_USERNAME=your_admin_username
WP_TEST_PASSWORD=your_admin_password

# WordPress URLs
WP_BASE_URL=http://localhost
WP_ADMIN_URL=http://localhost/wp-admin

# Database (if needed for direct DB tests)
DB_HOST=localhost
DB_NAME=wordpress
DB_USER=root
DB_PASSWORD=

# Redis (optional)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
